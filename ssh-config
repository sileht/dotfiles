    Hostname %hileht.net

Host gizmo
    AddressFamily inet
    DynamicForward 9999
    RemoteForward 62222 localhost:5555

Host eve
    AddressFamily inet
    DynamicForward 9898

Host eve-unlock
    Hostname 192.168.3.200
    ProxyCommand ssh -x nagios.t -W %h:%p
    RequestTTY yes
    RemoteCommand cryptroot-unlock
    Port 5555

Host rt
    Hostname 192.168.8.2


