###########
# DEFAULT #
###########

Host */*
    ProxyCommand ssh ${$(dirname %h)/\%%/@} nc ${$(basename %h)#*%%} %p

Host *
    ForwardX11 no
    ForwardAgent no
    Compression yes
    NoHostAuthenticationForLocalhost yes
    # PreferredAuthentications publickey,password,gssapi-keyex,gssapi-with-mic,
    #ControlPath ~/.ssh/master-%r@%h:%p
    #ControlMaster auto

