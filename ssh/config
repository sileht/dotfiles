Host alien billy gizmo eve bob rbob trudy
    ForwardAgent no
    RemoteForward /home/sileht/.var/S.gpg-agent /run/user/1000/gnupg/S.gpg-agent.extra
    RemoteForward /home/sileht/.var/S.gpg-agent.ssh /run/user/1000/gnupg/S.gpg-agent.ssh
    User sileht
    Port 5555

Host gizmo eve predator rbob rrt mergify
    Hostname %h.sileht.net

Host predator mergify rt rrt eve-unlock
   User root

Host gizmo
    AddressFamily inet
    DynamicForward 9999

Host eve
    AddressFamily inet
    DynamicForward 9898

Host eve-unlock
    Hostname 192.168.3.200
    ProxyCommand ssh -x nagios.t -W %h:%p
    RequestTTY yes
    RemoteCommand cryptroot-unlock
    Port 5555

Host mergify
   Port 2222

Host rt rrt
   Port 22222

Host rt
    Hostname 192.168.8.2

