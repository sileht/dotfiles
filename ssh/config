   Port 22222

Host alien billy gizmo eve bob rbob trudy predator
    RemoteForward /home/sileht/.var/S.gpg-agent /run/user/1000/gnupg/S.gpg-agent.extra
    RemoteForward /home/sileht/.var/S.gpg-agent.ssh /run/user/1000/gnupg/S.gpg-agent.ssh
    ForwardAgent yes
    User sileht
    Port 5555

Host gizmo eve predator rbob rrt mergify
    Hostname %h.sileht.net

Host gizmo
    AddressFamily inet
    DynamicForward 9999
    RemoteForward 62222 localhost:5555

Host eve
    AddressFamily inet
    DynamicForward 9898
    LocalForward 3000 localhost:3000

Host eve-unlock
    Hostname 192.168.3.200
    ProxyCommand ssh -x nagios.t -W %h:%p
    RequestTTY yes
    RemoteCommand cryptroot-unlock
    Port 5555

